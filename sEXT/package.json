{
  "name": "splitix",
  "displayName": "splitix",
  "description": "Splitix language support for VS Code and Visual Studio",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "splitix",
      "aliases": ["Splitix", "splitix"],
      "extensions": [".splt",".splib"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "splitix",
      "scopeName": "source.splt",
      "path": "./syntaxes/splitix.tmLanguage.json"
    }],
    "themes": [{
    
      "label": "Splitix Default Theme",
      "uiTheme": "vs-dark",
      "path": "./themes/color-theme.json"
    }]  
  }
}
