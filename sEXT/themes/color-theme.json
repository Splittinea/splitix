{
  "name": "Splitix Default Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editorCursor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#2a2a2a",
    "editor.selectionBackground": "#264f78"
  },
  "tokenColors": [
    {
      "name": "Splitix Script Controls",
      "scope": "keyword.scriptControl.splitix",
      "settings": {
        "foreground": "#5164ff"
      }
    },
    {
      "name": "Splitix Keywords",
      "scope": "keyword.control.splitix",
      "settings": {
        "foreground": "#946d40"
      }
    },
    {
      "name": "Splitix I/O",
      "scope": "keyword.io.splitix",
      "settings": {
        "foreground": "#9d5fa9"
      }
    },
    {
      "name": "Splitix Initialization",
      "scope": "keyword.initialization.splitix",
      "settings": {
        "foreground": "#a070ca"
      }
    },
    {
      "name": "Splitix Functions",
      "scope": "support.function.splitix",
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "name": "Splitix Strings",
      "scope": "string.quoted.double.splitix",
      "settings": {
        "foreground": "#6a9955"
      }
    },
    {
      "name": "Splitix Numbers",
      "scope": "constant.numeric.splitix",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Splitix Comments",
      "scope": "comment.line.splitix",
      "settings": {
        "foreground": "#6a6a6a",
        "fontStyle": "italic"
      }
    }
  ]
}